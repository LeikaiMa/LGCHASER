package scrambleString;

public class Recursive {
	public boolean isScramble(String s1, String s2) {
		if (s1 == null && s2 == null) {
			return true;
		}
		if (s1.isEmpty() && s2.isEmpty()) {
			return true;
		}
		if (s1.equals(s2)) {
			return true;
		}
		
		for (int i = 1; i < s1.length(); i++) {
			String s1l = s1.substring(0, i);
			String s1r = s1.substring(i);
			
		}
	}
}
