package nextNode;

import checkBalancedTree.TreeNode;

public class LeftMostChild {
	public TreeNode inoderSucc(TreeNode n) {
		if (n == null) {
			return null;
		}
		
		if (n.right != null) {
			return leftMostChild(n.right);
		}
	}
	
	
	
	
	public TreeNode leftMostChild(TreeNode n) {
		if (n == null) {
			return null;
		}
		
		while (n.left != null);
		return n;
	}
}
