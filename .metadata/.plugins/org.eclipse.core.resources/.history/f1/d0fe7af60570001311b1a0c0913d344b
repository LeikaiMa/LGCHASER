package regularExpressionMatching;

public class DFA {
	public  boolean isMatch(String s, String p) {
		if (s == null || p == null) {
			return false;
		}
		if (s.isEmpty() && p.isEmpty()) {
			return true;
		} else if (p.isEmpty() && !s.isEmpty()) {
			return false;
		}
		
		return isMatch(s, p, 0 ,0);
	}
}
