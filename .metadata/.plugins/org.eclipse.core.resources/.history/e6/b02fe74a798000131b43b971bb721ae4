package mergeSortedArray;

public class PutLargerElementBack {
    public void merge(int A[], int m, int B[], int n) {
        int index = m + n - 1;
        m--;
        n--;
        
        while (m >= 0 && n >= 0) {
            if (B[n] >= A[m]) {
                A[index--] = B[n--];
            } else {
                A[index--] = A[m--];
            }
        }
        
        while (n >= 0) {
            A[index--] = B[n--];
        }
    }
}
