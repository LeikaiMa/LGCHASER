package strStr;

public class Naive {
	public String strStr(String haystack, String needle) {
		int lenH = haystack.length();
		int lenN = needle.length();
		if (lenH == 0 && lenN == 0) {
			return haystack;
		}
		if (lenH < lenN) {
			return null;
		}
		
		if (lenH != 0 && lenN == 0) {
			return haystack;
		}
		
	}
}
