public class DFS {
	public int sumNumbers(TreeNode root) {
		if (root == null) {
			return 0;
		}

		ArrayList<Integer> results = new ArrayList<Integer>();
		StringBuffer number = new StringBuffer("");
		sumNumbersHelper(root, results, number);
		int sum = 0;
		for (int r : results) {
			sum += r;
		}
		return sum;
	}

