package decodeWays;

public class DecodeWays {
	public int numDecodings(String s) {
		if (s == null || s.isEmpty()) {
			return 0;
		}
		if (s.length() == 1) {
			return 1;
		}
		
		int [] num = new int[s.length() + 1];
		num[s.length()] = 1;
		num[s.length() -1] = 1;
		for (int i = s.length() -2; i>= 0; i++) {
			
		}
	}
}
