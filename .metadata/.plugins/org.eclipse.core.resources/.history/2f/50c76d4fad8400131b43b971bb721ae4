package findKthSmallest;

public class FixedValue {
	public int largestKSum(int[] num, int k) {
		int[] count = new int[101];
		for (int n : num) {
			count[n]++;
		}
		
		int sum = 0;
		int c = 0;
		for (int i = 1; i < 101 && c < k; i++) {
			c += count[i];
			if (c <= k) {
				sum += count[i] * i;
			}
		}
	}
}
