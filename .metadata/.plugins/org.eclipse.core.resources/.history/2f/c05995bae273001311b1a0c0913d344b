package permutationSequence;

import java.util.ArrayList;

public class DP {
	public String getPermutation(int n, int k) {
		int[] dp = new int[10];
		int total = 1;
		int digit = 9;
		for (int i = 1; i <= 9; i++) {
			total *= i;
			dp[i] = total;
		}
		ArrayList<Integer> numbers = new ArrayList<Integer>();
		for (int i = 1; i < 10; i++) {
			if (k > dp[i - 1] && k <= dp[i]) {
				numbers.add(i);
				digit = i;
				break;
			} else {
				numbers.add(i);
			}
		}
		
		StringBuffer sb = new StringBuffer();
		
		

	}
}
