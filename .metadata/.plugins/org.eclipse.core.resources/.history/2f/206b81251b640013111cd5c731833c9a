package placeQueens;

import java.util.ArrayList;

public class Queens {
	int GRID_SIZE = 8;
	public void placeQueens(int row, Integer[] columns, ArrayList<Integer[]> results) {
		if (row == GRID_SIZE) {
			results.add(columns.clone());
		} else {
			for (int col = 0; col < GRID_SIZE; col++) {
				if (checkValid(columns, row, col)) {
					columns[row] = col;
				}
			}
		}
	}
}
