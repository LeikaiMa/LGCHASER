package medianOfTwoSortedArray;

public class BruteForce {
	public double findMedianSortedArrays(int A[], int B[]) {
		if ((A == null || A.length == 0) && (B == null || B.length == 0)) {
			return 0;
		}
		int lenA;
		int lenB;
		if (A == null || A.length == 0) {
			lenB = B.length;
			if (lenB % 2 == 1) {
				return B[lenB >> 1];
			} else {
				return (B[(lenB + 1) >> 1] + B[lenB >> 1]) / 2.0
			}
		}
	}
}
