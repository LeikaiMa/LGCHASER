package sqrt;
// 这个是用牛顿迭代法，原本用的二分法，可能会溢出，可以加一个限定条件，
// 牛顿这个方法就记住就可以了。
public class Naive {
	public static  int sqrt(int x) {
		if (x == 0 || x == 1) {
			return x;
		}
		
		double last = 0;
		double res = 1;
		while (res != last) {
			last = res;
			res = (res + x / res) /2;
		}
		
		return (int) res;
		
		
		
	}
	
	public static void main(String[] args) {
		System.out.println(sqrt(2147395599) == 46339);
	}
}
