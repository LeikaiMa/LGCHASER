package runnableAndThread;

public class RunnableExample implements Runnable{
	public int count = 0;
	
	public void run() {
		System.out.println("RunnableThread starting.");
		try {
			while (count < 5) {
				Thread.sleep(500);
				count++;
			}
		} catch (InterruptedException exc) {
			// TODO: handle exception
		}
	}

}
