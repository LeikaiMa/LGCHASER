package leastCommonAncestorWithParent;

public class CountDepth {
	public TreeNode getLCA(TreeNode p, TreeNode q) {
		int h1 = getHeight(p);
		int h2 = getHeight(q);

		if (h1 < h2) {
			TreeNode tmp = p;
			p = q;
			q = tmp;
			int t = h1;
			h1 = h2;
			h2 = t;
		}
	}

	public int getHeight(TreeNode p) {
		int height = 0;
		while (p != null) {
			p = p.parent;
			height++;
		}
		return height;
	}
}
