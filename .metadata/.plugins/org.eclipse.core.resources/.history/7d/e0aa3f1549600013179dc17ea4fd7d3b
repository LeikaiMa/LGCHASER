package test;

import java.util.ArrayList;

public class TestArrayList {
	public static void main(String[] args) {
		ArrayList<String[]> al = new ArrayList<>();
		String[] board = new String[4];
		board[0] = ".Q..";
		board[1] = "...Q";
		board[2] = "Q...";
		
		al.add("\".Q..\",\"...Q\",\"Q...\",\"..Q.\"");
		
		System.out.println(al.toString());
		
	}

}
