package checkBalancedTree;

public class checkEntire {
	public static int getHeight(TreeNode root) {
		if (root == null) {
			return 0;
		}
		return Math.max(getHeight(root.left), getHeight(root.right)) + 1;
	}
	
	public static boolean isBalanced(TreeNode root) {
		if (root == null) {
			return true;
		}
	}
}
