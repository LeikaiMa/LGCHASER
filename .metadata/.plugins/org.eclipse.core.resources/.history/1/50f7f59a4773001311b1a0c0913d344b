package searchInRotatedArray;

public class CustomBinarySearch {
	public int search(int[] A, int target) {
		if (A == null || A.length == 0) {
			return -1;
		}
		int low = 0;
		int high = A.length - 1;
		while (low <= high) {
			int mid = low + (high - low) / 2;
			if (A[mid] == target) {
				return mid;
			} else if (target < A[mid]) {
				if (A[low] <= A[mid]) {
					high = mid - 1;
				} else {
					low = low + 1;
				}
			} else {
				
			}
		}
		return -1;
	}
}
