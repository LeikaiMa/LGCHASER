package bestTimeToBuyAndSellStockI;

public class ON {
	public int maxProfit(int[] prices) {
		if (prices == null || prices.length < 2) {
			return Integer.MIN_VALUE;
		}

		int buy = 0;
		int tmp = 0;
		int sell = 1;
		for (int i = 2; i < prices.length; i++) {
			if (prices[i] >= prices[sell]) {
				sell = i;
				if (i > tmp) {
					buy = tmp;
				}
			}
			if (prices[i])
		}
	}
}
