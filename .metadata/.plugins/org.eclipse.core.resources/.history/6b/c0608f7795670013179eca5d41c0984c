package rotateList;

public class Rotate {
	public ListNode rotateRight(ListNode head, int n) {
		if (head == null) {
			return null;
		}
		if (head.next == null) {
			return head;
		}
		int count = 1;
		ListNode runner = head;
		while (runner.next != null) {
			count++;
			runner = runner.next;
		}
		runner.next = head;
		int move = n % count;
		for (int i = 0; i < count -move; i++) {
			runner = runner.next;
		}
		
	}
}
