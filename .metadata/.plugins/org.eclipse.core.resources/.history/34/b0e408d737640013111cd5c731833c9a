package findElementInMatrix;

public class Coordinate implements Cloneable {
	public int row;
	public int column;

	public Coordinate(int r, int c) {
		row = r;
		column = c;
	}
	
	public boolean inbounds(int[][] matrix) {
		return row >= 0 && column >= 0 && row < matrix.length && column < matrix[0].length;
	}
}
