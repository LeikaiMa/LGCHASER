package insertionSortList;

public class InsertSort {
	public static  ListNode insertionSortList(ListNode head) {
		if (head == null || head.next == null) {
			return head;
		}

		ListNode dummy = new ListNode(0);
		dummy.next = head;

		ListNode next = dummy.next.next;

		while (next != null) {
			ListNode cur = dummy.next;
			ListNode prev = dummy;

			while (cur != next) {
				ListNode cNext = cur.next;
				if (cur.val > cNext.val) {
					prev.next = cNext;
					cur.next = cNext.next;
					cNext.next = cur;
				}

				cur = cur.next;
				prev = prev.next;
			}
			next = next.next;
		}
		return dummy.next;
	}
}
