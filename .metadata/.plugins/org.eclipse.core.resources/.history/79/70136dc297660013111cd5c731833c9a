package testString;
// string 之中也可以check 是不是一个字符串contain
public class ContainsCharacter {
	public static void main(String[] args) {
		String string = "aaa";
		String string2 = "**a";
		String string3 = "ab*";
		System.out.println(string.contains("*"));
		System.out.println(string2.contains("*"));
	}
}
