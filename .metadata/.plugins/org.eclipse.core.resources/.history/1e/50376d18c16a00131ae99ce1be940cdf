package singleSumII;

public class BitManipulation {
	public static int singleNumber(int[] A) {
		int one = 0;
		int two = 0;
		int three = 0;
		
		for (int i = 0; i < A.length; i++) {
			two |= one & A[i];
			one = one ^ A[i];
			three = ~(one & two);
			one = one & three;
			two = two & three;
		}
		
		return one;
	}
	
	public static void main(String[] args) {
		int [] A = {1,2,1,2,3,3,4,4,2,3,1,5,5,5,6,4};
		System.out.println(singleNumber(A));
	}
}
