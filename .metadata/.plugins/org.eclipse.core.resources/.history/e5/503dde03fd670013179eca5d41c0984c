package addBinary;

import java.util.ArrayList;

public class AddBinary {
	public String addBinary(String a, String b) {
		if (a == null || b== null) {
			return null;
		}
		String longer, shorter;
		if (a.length()>= b.length()) {
			longer = a;
			shorter = b;
		}
		ArrayList<Integer> result = new ArrayList<Integer>();
		
	}
}
