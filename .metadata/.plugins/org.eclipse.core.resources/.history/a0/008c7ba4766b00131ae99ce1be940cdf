package lengthOfLastWord;

public class Length {
	public int lengthOfLastWord(String s) {
		if (s == null || s.isEmpty()) {
			return 0;
		}
		int i = s.length() - 1;
		for (; i >= 0; i--) {
			if (s.charAt(i) != ' ') {
				break;
			}
		}
		
		if (i == -1) {
			return 0;
		}
		
		int num = 0;
		
		for (; i >= 0; i--) {
			num++;
		}
	}
}
