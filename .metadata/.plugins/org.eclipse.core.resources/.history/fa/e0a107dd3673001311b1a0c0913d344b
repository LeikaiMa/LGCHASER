package validParentheses;

import java.util.Stack;

public class StackWay {
	public boolean isValid(String s) {
		if (s == null || s.length() == 0) {
			return true;
		}
		if (s.length() % 2 == 1) {
			return false;
		}
		Stack<Character> paren = new Stack<>();

		for (int i = 0; i < s.length(); i++) {
			char c = s.charAt(i);
		}
	}
}
