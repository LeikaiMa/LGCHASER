package sqrt;

public class Naive {
	public static  int sqrt(int x) {
		if (x < 0) {
			return -1;
		} 
		if (x == 0) {
			return 0;
		}
		
		long low = 0;
		long high = x / 2 +1;
		while (low <= high) {
			int mid = low + (high -low) /2;
			int square = mid * mid;
			if ( square== x) {
				return mid;
			} else if (square > x) {
				high  = mid -1;
			} else {
				low = mid + 1;
			}
		}
		
		return high;
	}
	
	public static void main(String[] args) {
		System.out.println(sqrt(100));
	}
}
