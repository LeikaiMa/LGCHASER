package recursion;

public class Stairs {
	static int count = 0;
	public static int stairs (int n, int count) {
		if (n == 0) {
			
			return 1;
		} else if (n < 0) {
			return 0;
		} else {
			stairs(n - 1);
			stairs(n - 2);
			stairs(n - 3);
		}
	}
	public static void main(String[] args) {

	}

}
