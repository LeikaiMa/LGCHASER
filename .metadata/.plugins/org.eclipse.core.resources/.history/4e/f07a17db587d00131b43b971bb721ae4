package bestTimeToBuyAndSellStockII;

public class EarnAsMuchAsPossible {
	 public int maxProfit(int[] prices) {
	        int total = 0;
	        for (int i = 1; i < prices.length; i++) {
	            int profit = prices[i] - prices[i - 1];
	            total += profit > 0 ? profit : 0;
	        }
	        
	        return total;
	    }
}
