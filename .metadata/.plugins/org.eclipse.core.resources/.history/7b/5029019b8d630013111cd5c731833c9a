package rotatedArray;

public class customBinarySearch {
	public int search(int[] a, int left, int right, int x) {
		int mid = (left + right) / 2;
		if (x == a[mid]) {
			return mid;
		}
		if (right < left) {
			return -1;
		}
		
		if (a[left] < a[mid]) {
			if (x >= a[left] && x <= a[mid]) {
				return search(a, left, mid -1, x);
			} else {
				return search(a, mid+1, right, x)
			}
		}
	}
}
