package maximum;

public class ConsiderOverflow {
	public static int flip(int bit) {
		return 1 ^ bit;
	}

	public static int sign(int a) {
		return flip((a >> 31) & 0x1);
	}

	public static int getMax(int a, int b) {
		int c = a - b;
		int sa = sign(a);
		int sb = sign(b);
		int sc = sign(c);

	}
}
