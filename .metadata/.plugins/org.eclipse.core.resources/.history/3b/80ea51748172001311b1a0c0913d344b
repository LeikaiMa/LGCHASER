package sortColors;

public class OnePass {
	public void sortColors(int[] A) {
		if (A == null || A.length == 0 || A.length == 1) {
			return;
		}
		if (A.length == 2) {
			if (A[0] > A[1]) {
				swap(A, 0, 1);
			}
			return;
		}
		
		int r = 0;
		int w = 1;
		int b = A.length -1;
