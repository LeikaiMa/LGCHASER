package medianOfTwoSortedArray;

public class KthElement {
	public static double findMedianSortedArrays(int A[], int B[]) {
		int lenA = A.length;
		int lenB = B.length;
		int total = lenA + lenB;
		if (total % 2 == 1) {
			return findMedianSortedArraysHelper(A, 0, lenA, B, 0, lenB,
					total / 2);
		}
	}
}
