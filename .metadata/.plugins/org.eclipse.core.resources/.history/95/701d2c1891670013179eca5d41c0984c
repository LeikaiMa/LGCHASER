package buildBinaryTree;

public class InorderPreorder {
	public TreeNode buildTree(int[] preorder, int[] inorder) {
		if (inorder == null || postorder == null) {
			return null;
		}

		if (inorder.length != postorder.length) {
			return null;
		}
		if (inorder.length == 0) {
			return null;
		}
	}
}
