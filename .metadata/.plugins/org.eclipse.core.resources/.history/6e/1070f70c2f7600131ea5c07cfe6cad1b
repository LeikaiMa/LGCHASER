package philosophersDiningProblem;

public class Philosopher extends Thread {
	private int bites = 10;
	private Chopstick left;
	private Chopstick right;

	public Philosopher(Chopstick left, Chopstick right) {
		this.left = left;
		this.right = right;
	}

	public void eat() {
		pickUp();
		checw();
		putDown();
	}
	
	public void pickUp() {
		left.pickUp();
		right.pickUp();
	}
	
	public void chew() {
		
	}
	
	public void putDown() {
		
	}
}
