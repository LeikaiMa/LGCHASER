package removeDuplicateFromSortedArrayII;

public class TwoPointer {
	public int removeDuplicates(int[] A) {
		if (A == null || A.length == 0) {
			return 0;
		}
		if (A.length == 1) {
			return 1;
		}
		int slow = 0;
		int fast = 1;
		int count = 1;
		int size = 1;
		while (fast < A.length) {
			if (A[fast] == A[slow] && count < 2) {
				count++;
				size++;
				fast++;
				slow++;
			} else if (A[fast] == A[slow] && count >= 2) {
				fast++;
			} else {
				
			}
		}
	}
}
