package bestTimeToBuyAndSellStockI;

public class Clearer {
	public static int maxProfit(int[] prices) {
		int buy =0;
		int sell = 0;
		int min = 0;
		int maxdiff = 0;
		for (int i = 0; i < prices.length; i++) {
			if (prices[i] < min) {
				min = i;
			} else {
				int diff = prices[i] - prices[min];
				
			}
		}
	}
}
