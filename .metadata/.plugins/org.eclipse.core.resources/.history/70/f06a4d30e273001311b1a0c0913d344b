package permutationSequence;

public class DP {
	public String getPermutation(int n, int k) {
		int[] dp = new int[10];
		int total = 1;
		int digit = 9;
		for (int i = 1; i <= 9; i++) {
			total *= i;
			dp[i] = total;
		}
		
		for (int i = 0; i < 9; i++) {
			if (k > dp[i] && k < dp [i+1]) {
				
			}
		}
	}
}
