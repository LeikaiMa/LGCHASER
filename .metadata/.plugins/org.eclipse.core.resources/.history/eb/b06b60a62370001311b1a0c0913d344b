package reverseGroup;

public class TwoPointer {
	public ListNode reverseKGroup(ListNode head, int k) {
		if (head == null) {
			return head;
		}
		ListNode fast = head.next;
		ListNode slow = head;
		int i = 0;
		for (i = 0; i < k - 1; i++) {
			if (fast == null) {
				break;
			}
			slow = fast; 
			fast = fast.next;
		}
		
		if (i < k -1) {
			return head;
		}
	}
}
