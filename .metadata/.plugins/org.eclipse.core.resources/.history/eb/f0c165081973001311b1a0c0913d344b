package longestConsecutiveSequence;

import java.util.HashMap;

public class HashMapWithIndex {
	public int longestConsecutive(int[] num) {
		if (num == null) {
			return 0;
		}
		if (num.length == 0 || num.length == 1) {
			return num.length;
		}

