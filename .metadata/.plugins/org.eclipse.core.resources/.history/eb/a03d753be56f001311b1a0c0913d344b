package removeElement;

public class TwoPointer {
	public int removeElement(int[] A, int elem) {
		if (A == null || A.length == 0) {
			return 0;
		}
		
		int faster = 0;
		int slow = 0;
		while (faster < A.length) {
			if (A[faster] == elem) {
				faster++;
			} else {
				A[slow] = A[faster];
				slow++;
				faster++;
			}
		}
		return slow+ 1;
	}
}
