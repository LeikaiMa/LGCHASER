package nQueensII;

public class Permutation {
	public static int totalNQueens(int n) {
		if (n == 0 || n == 1) {
			return n;
		}
		int[] p = new int[n];
		for (int i = 0; i < n; i++) {
			p[i] = i;
		}
		
		int[] res = new int[1];
		res[0] = 0;
		permutation(p, 0, n-1, res);
		return res[0];
	}
	
	

}
