package countAndSay;

public class Loop {
	public String countAndSay(int n) {
		if (n <=0 ) {
			return null;
		}
		String s = "1";
		
		for (int i = 1 ; i < n; i++) {
			StringBuffer sb = new StringBuffer();
			char c = s.charAt(0);
			int count = 1;
			for (int j = 1; j < s.length(); j++) {
				if (s.charAt(j) == c) {
					count++;
				}
			}
		}
	}
}
