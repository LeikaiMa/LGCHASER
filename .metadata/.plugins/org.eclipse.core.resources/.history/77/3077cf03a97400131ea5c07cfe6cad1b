package validBinarySearchTree;

public class DFS {
	public boolean isValidBST(TreeNode root) {
		if (root == null) {
			return true;
		}

		TreeNode leftNode = root.left;
		TreeNode rightNode = root.right;
		if (root.left != null) {
			if (root.val <= leftNode.val) {
				return false;
			}
		}
		
		if (root.right != null) 
		{
			
		}
	}
}
