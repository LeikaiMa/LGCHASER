package bestTimeToBuyAndSellStockII;

public class ON {
	public int maxProfit(int[] prices) {
		int buy = 0;
		int sell = 0;
		int totalProfit = 0;

		for (int i = 0; i < prices.length; i++) {
			if (prices[i] <= prices[buy]) {
				if (prices[sell] > prices[buy]) {
					totalProfit += prices[sell] - prices[buy];
				}
				buy = i;
				sell = i;
			}
			if (prices[i] > prices[sell]) {
				
			}
		}
	}
}
