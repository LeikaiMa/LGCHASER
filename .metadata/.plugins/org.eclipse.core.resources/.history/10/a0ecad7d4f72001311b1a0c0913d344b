package testArray;

public class CeilAndFloorOfSortedArray {
	public static void main(String[] args) {
		int[] A = { 1, 2, 4, 4, 6, 6, 8 };
		int low = 0;
		int high = A.length - 1;
		int target = 5;
		while (low < high ){
			int mid = low + (high - low) / 2;
			if (A[mid] < A[low]) {
				low =  mid +1;
			} else {
				
			}
		}
	}
}
