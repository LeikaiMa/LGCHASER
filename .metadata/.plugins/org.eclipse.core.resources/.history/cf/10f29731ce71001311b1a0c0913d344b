package mergeTwoSortedLists;

public class DummyHead {
	public ListNode mergeTwoLists(ListNode l1, ListNode l2) {
		if (l1 == null && l2 == null) {
			return null;
		}
		if (l1 == null) {
			return l2;
		}

		if (l2 == null) {
			return l1;
		}
		
		ListNode dummy = new ListNode(0);
		ListNode prev = dummy;
	}
}
