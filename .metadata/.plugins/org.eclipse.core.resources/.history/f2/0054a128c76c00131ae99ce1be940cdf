package scrambleString;

public class Recursive {
	public boolean isScramble(String s1, String s2) {
		if (s1 == null && s2 == null) {
			return true;
		}
		if (s1.isEmpty() && s2.isEmpty()) {
			return true;
		}
		if (s1.length() == 1) {
			if (s1.charAt(0) == s1.charAt(0)) {
				return true;
			} else {
				return false;
			}
		}
	}
}
