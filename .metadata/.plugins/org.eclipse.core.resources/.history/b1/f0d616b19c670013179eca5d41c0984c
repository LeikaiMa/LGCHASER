package trapRain;

import java.util.ArrayList;

public class TrappingRainWater {
	public int trap(int[] A) {
		int max = 0;
		for(int i = 0; i < A.length; i++) {
			if (A[i] > max) {
				max = A[i];
			}
		}
		ArrayList<ArrayList<Integer>> heights = new ArrayList<ArrayList<Individual>>(4);heights[i] = new ArrayList<Integer>();
		}
			return 0;
	}
}
