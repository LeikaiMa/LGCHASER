package testBitManipulation;
// 这个说明的情况减号比 << 优先级要高。
public class Swap {

	public static void main(String[] args) {
		System.out.println((1<< 2) -1);
		int i = 0;
		int j = 1;
		i = i^j;
		j = i^j;
		i = i^j;
		
		System.out.println(i);
		System.out.println(j);
		
		int[] n= new int[2];
		n[0] = 0;
		n[1] = 1;
		n[0] = n[0] ^ n[1];
		n[1] = n[0] ^ n[1];
		n[0] = n[0] ^ n[1];
		System.out.println(n[0]);
		System.out.println(n[1]);
	}

}
