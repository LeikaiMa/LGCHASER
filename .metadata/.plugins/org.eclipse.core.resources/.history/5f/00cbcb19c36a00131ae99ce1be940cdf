package threeSumClosest;

import java.util.Arrays;

public class Closest {
	public int threeSumClosest(int[] num, int target) {
		Arrays.sort(num);
		int result = 0;
		int gap = Integer.MAX_VALUE;
		for (int i = 0; i < num.length -2; i ++) {
			for (int j = i + 1, k= num.length - 1; j < k; ) {
				int sum = num[j] + num[k];
			}
		}
	}
}
