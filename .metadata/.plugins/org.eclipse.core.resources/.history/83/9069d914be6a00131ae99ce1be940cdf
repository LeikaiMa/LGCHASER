package singleSumII;

public class SelectOneFromThree {
	public static int singleNumber(int[] A) {
		int[] bits = new int[32];
		for (int i = 0; i < 32; i++) {
			for (int j = 0; j < A.length; i++) {
				if (((A[j] >> i) & 1) == 1) {
					bits[i] = (bits[i] + 1) % 3;
				}
			}
		}
		int single = 0;
		for (int i = 31; i >= 0; i--) {
			single |= bits[i] << i;
		}
		return single;
	}
}
